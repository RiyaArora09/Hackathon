{
    "manifest_version": 3,
    "name": "CSAssistX",
    "version": "0.2",
    "background": {
    },

    "content_scripts": [
      {
        "matches": [
          "*://gamma.aviary.cs.amazon.dev/*"
        ],
        "js": [
          "content.js"
        ],
        "css":[
          "content.css"
        ]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "raptor.png"
    },
    "permissions": [
      "contextMenus",
      "storage",
      "tabs"
    ],
    "icons": {
      "16": "raptor.png"
    }
  }