{
    "manifest_version": 2,
    "name": "Clever Girl",
    "version": "0.2",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ],
        "css":[
          "content.css"
        ]
      }
    ],
    "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net; object-src 'self'",
    "browser_action": {
      "default_icon": "raptor.png",
      "default_title": "GPT-3 powered knowledge engine"
    },
    "permissions": [
      "contextMenus",
      "storage",
      "tabs",
      "http://127.0.0.1/"
    ],
    "icons": {
      "16": "raptor.png"
    }
  }